					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxEXPAND|wxLEFT</flag>
						<border>6</border>
						<object class="wxButton" name="ConvDialogOkButton">
							<label>OK</label>
							<default>0</default>
						</object>
					</object>
				</object>
